<!doctype html>
<html lang="en">
        <head>
                <meta charset="utf-8">
                <meta name="viewport" content="width=device-width, initial-scale=1">
                <meta name="description" content="CS411 Project">
                <meta name="author" content="Hammad Ali, Anna Liu, David Zhu">
                <title>CS411 Crime Database</title>
                <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css">

                <!-- Favicons -->
                <link rel="apple-touch-icon" href="/docs/5.0/assets/img/favicons/apple-touch-icon.png" sizes="180x180">
                <link rel="icon" href="/docs/5.0/assets/img/favicons/favicon-32x32.png" sizes="32x32" type="image/png">
                <link rel="icon" href="/docs/5.0/assets/img/favicons/favicon-16x16.png" sizes="16x16" type="image/png">
                <link rel="manifest" href="/docs/5.0/assets/img/favicons/manifest.json">
                <link rel="mask-icon" href="/docs/5.0/assets/img/favicons/safari-pinned-tab.svg" color="#7952b3">
                <link rel="icon" href="/docs/5.0/assets/img/favicons/favicon.ico">
                <meta name="theme-color" content="#7952b3">


                <style>
                        <!-- CSS goes here -->
                        ::selection {
                                background: rgba(255, 134, 0, 0.4);
                        }
                        ::-moz-selection {
                                background: rgba(255, 134, 0, 0.4);
                        }
                </style>

                <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
        </head>

        <body>

                <svg xmlns="http://www.w3.org/2000/svg" style="display: none;">
                        <symbol id="bootstrap" viewBox="0 0 118 94">
                                <title>Bootstrap</title>
                                <path fill-rule="evenodd" fill="currentColor" clip-rule="evenodd" d="M24.509 0c-6.733 0-11.715 5.893-11.492 12.284.214 6.14-.064 14.092-2.066 20.577C8.943 39.365 5.547 43.485 0 44.014v5.972c5.547.529 8.943 4.>
                        </symbol>

                        <symbol id="home" viewBox="0 0 16 16">
                                <path fill="currentColor" d="M8.354 1.146a.5.5 0 0 0-.708 0l-6 6A.5.5 0 0 0 1.5 7.5v7a.5.5 0 0 0 .5.5h4.5a.5.5 0 0 0 .5-.5v-4h2v4a.5.5 0 0 0 .5.5H14a.5.5 0 0 0 .5-.5v-7a.5.5 0 0 0-.146-.354L13 5.793V2.5a.5.5 >
                        </symbol>

                        <symbol id="speedometer2" viewBox="0 0 16 16">
                                <path fill="currentColor" d="M8 4a.5.5 0 0 1 .5.5V6a.5.5 0 0 1-1 0V4.5A.5.5 0 0 1 8 4zM3.732 5.732a.5.5 0 0 1 .707 0l.915.914a.5.5 0 1 1-.708.708l-.914-.915a.5.5 0 0 1 0-.707zM2 10a.5.5 0 0 1 .5-.5h1.586a.5.5>
                                <path fill="currentColor" fill-rule="evenodd" d="M0 10a8 8 0 1 1 15.547 2.661c-.442 1.253-1.845 1.602-2.932 1.25C11.309 13.488 9.475 13 8 13c-1.474 0-3.31.488-4.615.911-1.087.352-2.49.003-2.932-1.25A7.988 7.9>
                        </symbol>

                        <symbol id="table" viewBox="0 0 16 16">
                                <path fill="currentColor" d="M0 2a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v12a2 2 0 0 1-2 2H2a2 2 0 0 1-2-2V2zm15 2h-4v3h4V4zm0 4h-4v3h4V8zm0 4h-4v3h3a1 1 0 0 0 1-1v-2zm-5 3v-3H6v3h4zm-5 0v-3H1v2a1 1 0 0 0 1 1h3zm-4-4h>
                        </symbol>

                        <symbol id="grid" viewBox="0 0 16 16">
                                <path fill="currentColor" d="M1 2.5A1.5 1.5 0 0 1 2.5 1h3A1.5 1.5 0 0 1 7 2.5v3A1.5 1.5 0 0 1 5.5 7h-3A1.5 1.5 0 0 1 1 5.5v-3zM2.5 2a.5.5 0 0 0-.5.5v3a.5.5 0 0 0 .5.5h3a.5.5 0 0 0 .5-.5v-3a.5.5 0 0 0-.5-.5h-3>
                        </symbol>

                        <symbol id="people-circle" viewBox="0 0 16 16">
                                <path fill="currentColor" d="M11 6a3 3 0 1 1-6 0 3 3 0 0 1 6 0z"/>
                                <path fill="currentColor" fill-rule="evenodd" d="M0 8a8 8 0 1 1 16 0A8 8 0 0 1 0 8zm8-7a7 7 0 0 0-5.468 11.37C3.242 11.226 4.805 10 8 10s4.757 1.225 5.468 2.37A7 7 0 0 0 8 1z"/>
                        </symbol>

                        <symbol id="database" viewBox="0 0 16 16">
                                <path fill-rule="evenodd" fill="currentColor" clip-rule="evenodd" d="M4.318 2.687C5.234 2.271 6.536 2 8 2s2.766.27 3.682.687C12.644 3.125 13 3.627 13 4c0 .374-.356.875-1.318 1.313C10.766 5.729 9.464 6 8 6s-2.>
                        </symbol>

                        <symbol id="info" viewBox="0 0 16 16">
                                <path fill="currentColor" d="M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14zm0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16z"/>
                                <path fill="currentColor" d="m8.93 6.588-2.29.287-.082.38.45.083c.294.07.352.176.288.469l-.738 3.468c-.194.897.105 1.319.808 1.319.545 0 1.178-.252 1.465-.598l.088-.416c-.2.176-.492.246-.686.246-.275 0-.375-.>
                        </symbol>

                        <symbol id="map" viewBox="0 0 16 16">
                                <path fill="currentColor" d="M8 16s6-5.686 6-10A6 6 0 0 0 2 6c0 4.314 6 10 6 10zm0-7a3 3 0 1 1 0-6 3 3 0 0 1 0 6z"/>
                        </symbol>
                </svg>

                <main>
                        <!-- Use viewport height adjustment to fill screen space. For modern browsers only -->
                        <div class="container-fluid">
                                <div class = "row flex-nowrap">
                                        <div class="d-flex flex-column flex-shrink-0 p-3 min-vh-100 text-white bg-dark" style="width:280px;">
                                                <a class="d-flex align-items-center mb-3 mb-md-0 me-md-auto text-white text-decoration-none">
                                                        <svg class="bi me-2" width="40" height="40"><use xlink:href="#database"/></svg>
                                                        <span class="fs-4">CS411 Project</span>
                                                </a>

                                                <hr>
                                                <ul class="nav nav-pills flex-column mb-auto">
                                                        <li class="nav-item">
                                                                <a href="#" class="nav-link active" aria-current="page">
                                                                        <svg class="bi me-2" width="16" height="16"><use xlink:href="#home"/></svg>
                                                                        Home
                                                                </a>
                                                        </li>

                                                        <li>
                                                                <a href="AboutPage.html" class="nav-link text-white">
                                                                        <svg class="bi me-2" width="16" height="16"><use xlink:href="#info"/></svg>
                                                                        About
                                                                </a>
                                                        </li>

                                                        <li>
                                                                <a href="#" class="nav-link text-white">
                                                                        <svg class="bi me-2" width="16" height="16"><use xlink:href="#map"/></svg>
                                                                        Map
                                                                </a>
                                                        </li>

                                                        <li>
                                                                <a href="#" class="nav-link text-white">
                                                                        <svg class="bi me-2" width="16" height="16"><use xlink:href="#people-circle"/></svg>
                                                                        Empty
                                                                </a>
                                                        </li>
                                                </ul>

                                        </div>
                                        <div class = "col py-3" style="background-color: #f8f9fa;">
                                                <h2 class = "" style="margin-left: 1px; margin-bottom: 17px;">Home</h2>
                                                <hr>
                                                <p class=""> This is the home page and will contain the main functions of our database project </p>
                                                <br>
                                                <br>

                                                <form class="d-flex px-5" role="search" action="search" method="POST">
                                                        <input name = "idSearchBar" id="idSearchBar" class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                                                        <button id="idSearchButton" class="btn btn-outline-primary" type="submit">Search</button>
                                                </form>
                                                <form class="d-flex px-5" role="add" action="add" method="POST">
                                                        <input name = "idAddBar" id="idAddBar" class="form-control me-2" type="search" placeholder="Insert" aria-label="Insert">
                                                        <button id="idAddButton" class="btn btn-outline-secondary ms-1" type="submit">Add</button>
                                                </form>
                                                <form class="d-flex px-5" role="remove" action="remove" method="POST">
                                                        <input name = "idRemoveBar" id="idRemoveBar" class="form-control me-2" type="search" placeholder="Remove" aria-label="Remove">
                                                        <button id="idRemoveButton" class="btn btn-outline-danger ms-1" type="submit">Remove</button>
                                                </form>
                                                <form class="d-flex px-5" role="update" action="update" method="POST">
                                                        <input name = "idUpdateBar" id="idUpdateBar" class="form-control me-2" type="search" placeholder="Update" aria-label="Update">
                                                        <button id="idUpdateButton" class="btn btn-outline-danger ms-1" type="submit">Update</button>
                                                </form>
                                                <form class="d-flex px-5" role="aq1" action="aq1" method="POST">
                                                        <button id="idAq1Button" class="btn btn-outline-danger ms-1" type="submit">AdvQ1: GROUP BY and JOIN</button>
                                                </form>
                                                <form class="d-flex px-5" role="aq2" action="aq2" method="POST">
                                                        <button id="idAq2Button" class="btn btn-outline-danger ms-1" type="submit">AdvQ2: Set operations, Subqueries, JOIN</button>
                                                </form>
                                                <br>
                                                <p><%= command %></p>
                                                <br>
                                                <p>Length: <%= data.length %></p>
                                                <br>
                                                <ul>
                                                  <% if (data) { %>
                                                  <% for (var i = 0; i < 10; i++) { %>
                                                    <li><%= data[i].CrimeID %> - <%= data[i].LocationID %> - <%= data[i].VictimID %> - <%= data[i].TypeID %> - <%= data[i].Time %></li>
                                                  <% } %>
                                                  <% } %>
                                                </ul>
                                        </div>
                                </div>
                        </div>
                </main>
        </body>
</html>
